<?xml version="1.0" encoding="UTF-8"?>
<coCoMM:FeatureModel
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:coCoMM="http://www.example.org/coCoMM"
    xsi:schemaLocation="http://www.example.org/coCoMM coCoMM.ecore"
    name="AWS">
  <root id="EC2"
      name="EC2">
    <treeConstraints>
      <children
          id="OS"
          name="OS">
        <treeConstraints
            type="Alternative">
          <children
              id="Windows"
              name="Windows">
            <featureAttributes
                xsi:type="coCoMM:DependantFeatureAttribute"
                name="Production"
                value="0.0065"
                fixedType="Cost"
                variableValue="200"
                varType="TimeVar"/>
          </children>
          <children
              id="Linux"
              name="Linux">
            <featureAttributes
                xsi:type="coCoMM:DependantFeatureAttribute"
                name="Production"
                value="0.005"
                fixedType="Cost"
                variableValue="200"
                varType="TimeVar"/>
          </children>
        </treeConstraints>
      </children>
      <children
          id="Type"
          name="Type">
        <treeConstraints
            type="Alternative">
          <children
              id="t2"
              name="t2">
            <featureAttributes
                name="Production"
                value="0.001"
                fixedType="Cost"/>
          </children>
          <children
              id="m3"
              name="m3">
            <featureAttributes
                name="Production"
                value="0.001"
                fixedType="Cost"/>
          </children>
        </treeConstraints>
      </children>
    </treeConstraints>
  </root>
  <attributeTypes
      id="Cost"
      name="Cost"
      dataType="double"
      unitMeasure="USD"/>
  <attributeTypes
      id="TimeVar"
      name="Time"
      variable="true"
      unitMeasure="hours"/>
  <ctConstraints
      from="Windows"
      to="t2"
      type="excludes"/>
</coCoMM:FeatureModel>
